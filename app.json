{
  "expo": {
    "name": "Expense tracker",
    "slug": "expense-tracker2",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon/Expense_Tracker_Icon.png",
    "scheme": "expensetracker2",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "entryPoint": "./index.js",
    "ios": {
      "supportsTablet": true,
      "package": "com.salvatusdev.expensetracking",
      "bundleIdentifier": "com.salvatusdev.expensetracking",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.salvatusdev.expensetracking",
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon/Expense_Tracker_Icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/icon/Expense_Tracker_Icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "30c7996c-310b-4988-87a1-fbc94ffdbecf"
      }
    }
  }
}
